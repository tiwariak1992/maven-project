pipeline
{
  agent any
  stages
  {
    stage('scm checkout')
    {
      steps
      {
        git'https://github.com/tiwariak1992/maven-project.git'
      }
    }
    stage('maven compile')
    {
      parallel
      {
        stage('maven package goal')
        {
          steps
          {
            sh 'mvn package'
          }
        }
        stage('maven install')
        {
          sh 'mvn install'
        }
      }
    }
  }
  failfast:true
}
